<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	
	<bean id="id1" class="Student" primary="true">
		<property name="name" value="Srinu"></property>
		<property name="address" value="Rajampalli"></property>
	</bean>
	
	<bean id="id2" class="Student">
		<property name="name" value="katta"></property>
		<property name="address" value="Darsi"></property>
	</bean>
	
	
</beans>